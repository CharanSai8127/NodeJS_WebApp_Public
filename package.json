{
  "name": "basic-nodejs",
  "version": "1.0.0",
  "main": "app.js",
  "description": "NodeJS Project with Express Framework and Running in Docker Container",
  "author": "Aditya",
  "license": "MIT",
  "dependencies": {
    "express": "*",
    "mysql": "^2.18.1"
  },
  "devDependencies": {},
  "scripts": {
    "start": "node app.js",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest"
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit"
